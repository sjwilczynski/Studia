#include <stdio.h>

long l_max = 0x7FFFFFFFFFFFFFFF;
long l_min = 0x8000000000000000;

long adds(long x, long y)
{
	long res;
	asm("addq %%rdi, %%rsi;"
		"movq %%rsi, %0;"
		"movq l_max, %%rcx;"
		"cmovnsq l_min, %%rcx;"
		"cmovoq %%rcx, %0"
		: "=r"(res)         
		:                    
		: "rcx","rdi","rsi"  
		);
	return res;
}


int main()
{
	long l1,l2;
	scanf("%ld %ld",&l1,&l2);
	long result = adds(l1,l2);		
	printf("%ld\n",result);
	return 0;
}
